#lang rhombus/static

import:
  lib("racket/base.rkt") only #{make-immutable-hashalw}
  lib("json/exp.rkt") ! #{for-extension} as rkt_json:
    rename #{read-json*} as read

export:
  read

fun read(in :: Port.Input):
  rkt_json.read("json.read",
                in,
                #'null,
                base.#{make-immutable-hashalw},
                PairList.to_list,
                String.snapshot,
                String.snapshot)
