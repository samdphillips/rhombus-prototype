#lang rhombus

import:
  rhombus/macro open
  "typeset-example.rkt" open

export:
  examples
  make_rhombus_eval
  close_eval

expr.macro '(examples $parens $tail ......):
  ~op_stx: me
  match parens
  | '($_, ...):
      values(parsed(['#{typeset-examples}, parens]),
             tail)
  | ~else:
      raise_syntax_error("expected forms", '($me $parens))

